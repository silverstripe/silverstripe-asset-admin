{
  "name": "silverstripe/asset-admin",
  "description": "Asset management for the SilverStripe CMS",
  "type": "silverstripe-module",
  "require": {
    "php": ">=5.5.0",
    "silverstripe/cms": "^4",
    "silverstripe/framework": "^4"
  },
  "require-dev": {
    "phpunit/PHPUnit": "~4.8",
    "silverstripe/behat-extension": "^2.1.0",
    "silverstripe/serve": "dev-master",
    "silverstripe/testsession": "^2.0.0-alpha3",
    "se/selenium-server-standalone": "2.41.0"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "autoload": {
    "psr-4": {
      "SilverStripe\\AssetAdmin\\": "code/",
      "SilverStripe\\AssetAdmin\\Tests\\": "tests/php/",
      "SilverStripe\\AssetAdmin\\Tests\\Behat\\": "tests/behat/src/"
    }
  },
  "config": {
    "process-timeout": 600
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
